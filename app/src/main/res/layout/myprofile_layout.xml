<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="wrap_content">

    <com.example.myapplication_kakao.com.example.myapplication_kakao.model.CustomViewCircle
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@mipmap/ic_launcher"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"/>






    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/textView_name"
        android:paddingTop="15dp"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/imageView"
        android:paddingLeft="10dp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="status"
        android:id="@+id/textView_statusMsg"
        android:layout_marginLeft="233dp"
        android:layout_alignBottom="@+id/textView_name"
        android:layout_alignParentEnd="true"
        android:background="@drawable/sgray"
        android:paddingTop="5dp"
        android:paddingRight="5dp"
        android:paddingLeft="15dp" />



</RelativeLayout>
